<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:shell="clr-namespace:BetaSharp.Launcher.Shell"

        x:Class="BetaSharp.Launcher.Shell.ShellView" x:DataType="shell:ShellViewModel"

        Icon="../../logo.ico" Title="BetaSharp Launcher"
        Width="850" Height="480" MinWidth="850" MinHeight="480" WindowStartupLocation="CenterScreen">

    <TransitioningContentControl Content="{CompiledBinding Current}">
        <TransitioningContentControl.PageTransition>
            <CrossFade Duration="00:00:00.5" />
        </TransitioningContentControl.PageTransition>
    </TransitioningContentControl>
</Window>
